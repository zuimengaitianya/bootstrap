<!DOCTYPE html>
<html lang="zh-cn" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>后台管理-@ViewBag.Title</title>
    <link rel="bootstrap icon" href="../Images/bootstrapicon.ico" />
    <link href="~/Content/jquery-ui-1.11.2/jquery-ui.css" rel="stylesheet" />
    <link href="../Content/bootstrap-3.3.4-dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="../Content/bootstrap-3.3.4-dist/css/bootstrap-theme.min.css" rel="stylesheet" />
    <link href="../Content/bootstrap/bootstrap.selfstyle.css" rel="stylesheet" />
    <link href="~/Scripts/css/mytips.css" rel="stylesheet" />
    <script src="../Scripts/jquery-1.11.3.min.js"></script>
    <script src="~/Scripts/common/jquery.cookie.js"></script>
    <script src="../Content/bootstrap-3.3.4-dist/js/bootstrap.min.js"></script>
    <script src="~/Content/jquery-ui-1.11.2/jquery-ui.js"></script>
    <script src="~/Scripts/common/mytips.js"></script>
    <script src="~/Scripts/common/jsUtil.js"></script>
    <script src="~/Scripts/common/Form-utils.js"></script>
    <script src="~/Scripts/common/DateCommon.js"></script>
    <script src="~/Scripts/common/BaseDictCommon.js"></script>
    <script type="text/javascript">
    //手风琴图标变换
    function changeMenuIcon(o) {
        if (o.childNodes[0].className == "glyphicon glyphicon-chevron-down") {
            $(o.childNodes[0]).removeClass("glyphicon glyphicon-chevron-down");
            $(o.childNodes[0]).addClass("glyphicon glyphicon-chevron-right");
        }
        else {
            $(o.childNodes[0]).removeClass("glyphicon glyphicon-chevron-right");
            $(o.childNodes[0]).addClass("glyphicon glyphicon-chevron-down");
        }
    }
</script>
</head>
<body data-spy="scroll" data-target=".bs-docs-sidebar">

    <div class="navbar navbar-inverse navbar-fixed-top" style="background-color:#e5e5e5;">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Bootstrap</a>
            </div>
            <div id="navbar" class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="#">Home</a></li>
                    <li><a href="#about">About</a></li>
                    <li><a href="#contact">Contact</a></li>
                </ul>
            </div>
        </div>
    </div>
    
    <header class="jumbotron subhead" id="overview">
        <div class="container">
            <h1>脚手架</h1>
            <p class="lead">Bootstrap建立在12列栅格系统、布局、组件之上。</p>
        </div>
    </header>

    <div class="container">
        <div class="row">
            <div class="col-md-3">
                <span class="btn btn-block" data-toggle="collapse" data-target="#menusecond" style="border: 1px solid #e5e5e5; text-align: left;" onclick="changeMenuIcon(this)"><span class="glyphicon glyphicon-chevron-right" style="font-weight: 600;">基本设置</span></span>
                <div id="menusecond" class="collapse in">
                    <div class="accordion-inner" style="padding: 0px;">
                        <div class="list-group">
                            <a href="/OrgDict/Index" class="list-group-item">机构设置<span class="glyphicon glyphicon-chevron-right" style="float: right;"></span></a>
                            <a href="/DeptDict/Index" class="list-group-item">科室设置<span class="glyphicon glyphicon-chevron-right" style="float: right;"></span></a>
                            <a href="/RoleDict/Index" class="list-group-item">角色设置<span class="glyphicon glyphicon-chevron-right" style="float: right;"></span></a>
                            <a href="/User/Index" class="list-group-item">用户设置<span class="glyphicon glyphicon-chevron-right" style="float: right;"></span></a>
                            <a href="/BaseDict/Index" class="list-group-item">基础字典设置<span class="glyphicon glyphicon-chevron-right" style="float: right;"></span></a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-9">
                <section>
                    @RenderBody()
                </section>
                @*@Scripts.Render("~/bundles/jquery")*@
                @RenderSection("scripts", required: false)
            </div>
        </div>
    </div>
</body>
</html>

